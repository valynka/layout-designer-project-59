section.contacts.tab-pane.fade.show.active(id='nav-contacts' role='tabpanel' aria-labelledby='nav-contacts-tab')
  h1.h2.fw-bold.mb-4 Контакты
  input.px-4.py-3.border-0.border-bottom.w-100(type='text' placeholder='Поиск по контактам')
  each users, letter in contacts
    p.text-muted.text-uppercase.mt-5= letter 
    each user in users
      a.contact.mb-3(href='#')          
        img.contact-img(src= user.avatar alt="avatar")
        .contact-body
          p.contact-name= user.name
          p.contact-status= user.status
